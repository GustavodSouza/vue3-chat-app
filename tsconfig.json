{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "#q-app/*": [".quasar/*"]
    }
  },
  "include": [".quasar", "src/**/*"],
  "exclude": ["node_modules", "dist"]
}
